  <template type="public" title="sv_ue_member_username_changes" version_id="3000010" version_string="3.0.0 Alpha"><![CDATA[<xf:title>{{ phrase('sv_ue_previous_names:') }} {$user.username}</xf:title>

<div class="block">
	<div class="block-container">
		<div class="block-body">
			<xf:datalist data-xf-init="responsive-data-list">
				<xf:datarow rowtype="header">
					<xf:cell>{{ phrase('sv_ue_from_name') }}</xf:cell>
					<xf:cell>{{ phrase('sv_ue_to_name') }}</xf:cell>
					<xf:cell>{{ phrase('date') }}</xf:cell>
					<xf:if is="$xf.visitor.canViewUsernameChangeDetails($user.user_id)">
						<xf:cell>{{ phrase('status') }}</xf:cell>
					</xf:if>
				</xf:datarow>

				<xf:foreach loop="$usernameChanges" value="$usernameChange">
					<xf:datarow>
						<xf:cell>{$usernameChange.from_username}</xf:cell>
						<xf:cell>{$usernameChange.to_username}</xf:cell>
						<xf:cell><xf:date time="{$usernameChange.date}" /></xf:cell>
						<xf:if is="$xf.visitor.canViewUsernameChangeDetails($user.user_id)">
							<xf:cell href="{{ link('members/username-change', '', {'unc_id': $usernameChange.unc_id}) }}"
								overlay="true">
								<xf:if is="$usernameChange.status == 'approved'">
									{{ phrase('sv_ue_approved') }}
								<xf:elseif is="$usernameChange.status == 'denied'" />
									{{ phrase('rejected') }}
								<xf:else />
									{{ phrase('pending') }}
								</xf:if>
							</xf:cell>
						</xf:if>
					</xf:datarow>
				</xf:foreach>
			</xf:datalist>
		</div>

		<div class="block-footer">
			<ul class="listInline listInline--bullet">
				<li>{{ phrase('sv_ue_total_changes:') }} {{ count($usernameChanges)|number }}</li>
			</ul>
		</div>
	</div>
</div>
]]></template>
