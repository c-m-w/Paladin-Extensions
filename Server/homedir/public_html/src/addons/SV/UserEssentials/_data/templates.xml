  <template type="public" title="sv_ue_username_change_info" version_id="3000041" version_string="3.0.0 Beta 11"><![CDATA[<xf:title>{{ phrase('sv_ue_user_name_change_for', {'name': $user.username}) }}</xf:title>

<div class="form block">
	<div class="block-container">
		<div class="block-body">
			<xf:formrow label="{{ phrase('name') }}">
				{$usernameChange.to_username}
			</xf:formrow>

			<xf:formrow label="{{ phrase('sv_ue_requester') }}">
				<ul class="listInline listInline--bullet">
					<li><xf:username user="$user" username="{$usernameChange.from_username}" rich="true" /></li>
					<xf:if is="$xf.visitor.canViewIps() and $usernameChange.ip_address">
						<li><a href="{{ link('misc/ip-info', null, {'ip': $usernameChange.ip_address|ip}) }}" target="_blank">
							{$usernameChange.ip_address|ip}
						</a></li>
					</xf:if>
					<li><xf:date time="{$usernameChange.date}" /></li>
				</ul>

				<xf:if is="$usernameChange.user_note">
					<div class="u-muted">{$usernameChange.user_note}</div>
				</xf:if>
			</xf:formrow>

			<xf:formrow label="{{ phrase('status') }}">
				<xf:if is="$usernameChange.status == 'approved'">
					{{ phrase('sv_ue_approved') }}
				<xf:elseif is="$usernameChange.status == 'denied'" />
					{{ phrase('rejected') }}
				<xf:else />
					{{ phrase('pending') }}
				</xf:if>
			</xf:formrow>

			<xf:if is="$xf.visitor.canApproveRejectUsername() and $moderator">
				<xf:formrow label="{{ phrase('moderator') }}">
					<ul class="listInline listInline--bullet">
						<li><xf:username user="$moderator" rich="true" /></li>
						<li><xf:date time="{$usernameChange.moderation_date}" /></li>
					</ul>

					<xf:if is="$usernameChange.moderator_note">
						<div class="u-muted">{$usernameChange.moderator_note}</div>
					</xf:if>
				</xf:formrow>
			</xf:if>
		</div>

		<xf:if is="$usernameChange.canEditPrivacy()">
			<xf:form action="{{ link('members/username-change', $user, {'unc_id': $usernameChange.unc_id}) }}">
				<xf:checkboxrow>
					<xf:option label="{{ phrase('private') }}" name="is_private" selected="{$usernameChange.is_private}" />
					<xf:explain>{{ phrase('sv_ue_private_explain') }}</xf:explain>
				</xf:checkboxrow>

				<xf:submitrow icon="save" />
				<xf:redirect url="{$redirect}" />
			</xf:form>
		<xf:elseif is="$usernameChange.is_private && $xf.options.useressDisplayProfileTab !== 'nobody'" />
				<xf:checkboxrow>
					<xf:option label="{{ phrase('private') }}" name="is_private" selected="{$usernameChange.is_private}" readonly="readonly" disabled="disabled" />
					<xf:explain>{{ phrase('sv_ue_private_explain') }}</xf:explain>
				</xf:checkboxrow>
		</xf:if>
	</div>
</div>]]></template>
