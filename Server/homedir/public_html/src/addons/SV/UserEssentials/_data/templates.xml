<?xml version="1.0" encoding="utf-8"?>
<templates>
  <template type="admin" title="sv_ue_log_login_attempt_list" version_id="3000037" version_string="3.0.0 Beta 7"><![CDATA[<xf:title>{{ phrase('sv_ue_login_attempt_log') }}</xf:title>

<xf:form action="{{ link('logs/login-attempt') }}" class="block">
	<div class="block-container">
		<div class="block-body block-row">
			<xf:textbox name="login" placeholder="{{ phrase('sv_ue_login...') }}" type="search"
				value="{{ $linkFilters.login }}"
				class="input--inline" />
			<xf:textbox name="ip_address" placeholder="{{ phrase('ip_address...') }}" type="search"
				value="{{ $linkFilters.ip_address }}"
				class="input--inline" />
			<xf:button type="submit">{{ phrase('show') }}</xf:button>
		</div>
	</div>
</xf:form>

<xf:if is="$entries is not empty">
	<div class="block">
		<div class="block-container">
			<div class="block-body">
				<xf:datalist data-xf-init="responsive-data-list">
					<xf:datarow rowtype="header">
						<xf:cell>{{ phrase('sv_ue_login') }}</xf:cell>
						<xf:cell>{{ phrase('ip_address') }}</xf:cell>
						<xf:cell>{{ phrase('date') }}</xf:cell>
						<xf:if is="$xf.visitor.hasAdminPermission('user')">
							<xf:cell class="dataList-cell--min">{{ phrase('delete') }}</xf:cell>
						</xf:if>
					</xf:datarow>

					<xf:foreach loop="$entries" value="$entry">
						<xf:datarow>
							<xf:cell><a href="{{ link('logs/login-attempt', '', {'login': $entry.login}) }}">{$entry.login}</a></xf:cell>
							<xf:cell class="u-ltr">
								<a href="{{ link('logs/login-attempt', '', {'ip_address': $entry.ip_address|ip}) }}">
									{{ $entry.ip_address|ip }}
								</a>
							</xf:cell>
							<xf:cell><xf:date time="{$entry.attempt_date}" /></xf:cell>
							<xf:if is="$entry.canDelete()">
								<xf:delete href="{{ link('logs/login-attempt', $entry) }}" />
							</xf:if>
						</xf:datarow>
					</xf:foreach>
				</xf:datalist>
			</div>

			<div class="block-footer">
				<span class="block-footer-counter">{{ display_totals($entries, $total) }}</span>
			</div>
		</div>

		<xf:pagenav page="{$page}" perpage="{$perPage}" total="{$total}"
			link="logs/login-attempt" params="{$linkFilters}"
			wrapperclass="block-outer block-outer--after" />
	</div>
<xf:else />
	<div class="blockMessage">{{ phrase('no_entries_have_been_logged') }}</div>
</xf:if>
]]></template>
  <template type="admin" title="sv_ue_log_login_attempt_view" version_id="3000010" version_string="3.0.0 Alpha"><![CDATA[<xf:title>{{ phrase('sv_ue_login_attempt') }}</xf:title>

<div class="block">
	<div class="block-container">
		<div class="block-body">
			<xf:formrow label="{{ phrase('sv_ue_login') }}">
				<a href="{{ link('logs/login-attempt', '', {'login': $entry.login}) }}">{$entry.login}</a>
			</xf:formrow>

			<xf:formrow label="{{ phrase('ip_address') }}">
				<a href="{{ link('logs/login-attempt', '', {'ip_address': $entry.ip_address|ip}) }}">{{ $entry.ip_address|ip }}</a>
			</xf:formrow>

			<xf:formrow label="{{ phrase('date') }}">
				<xf:date time="{$entry.attempt_date}" />
			</xf:formrow>
		</div>

		<xf:if is="$entry.canDelete()">
			<xf:form action="{{ link('logs/login-attempt', $entry) }}">
				<xf:checkboxrow>
					<xf:option name="delete_login" value="1">{{ phrase('sv_ue_clear_all_login_attempts_for_login') }}</xf:option>
					<xf:option name="delete_ip" value="1">{{ phrase('sv_ue_clear_all_login_attempts_for_ip') }}</xf:option>
				</xf:checkboxrow>
				<xf:redirect url="{$redirect}" />
				<xf:submitrow icon="delete" />
			</xf:form>
		</xf:if>
	</div>
</div>
]]></template>
